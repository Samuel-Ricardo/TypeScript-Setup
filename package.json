{
  "name": "typescript-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --watch ./src/ --exec ts-node ./src/ -e ts",
    "husky:prepare": "husky install",
    "husky:setup-hooks": "npx husky add .husky/pre-commit 'npx lint-staged'",
    "lint": "eslint ./src/** --fix",
    "pretty": "prettier --write --check ./src/**",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint",
    "test": "node --test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^18.16.1",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "prettier": "^2.8.8"
  }
}
